<process plugin-version="1.0.0" plugin-api-version="@PLUGIN_API_VERSION@" jips-version="@JIPS_VERSION@">
  <name>maskprocess</name>
  <defaults>
    <default id="size" value="3"/>
  </defaults>
  <inputmask>
    <input type="label" translate="false" bounds="0,0,2,1" id="label1">
      <text>Name:</text>
    </input>
    <input type="textfield" translate="false" bounds="2,0,4,1" id="name">
      <text>default</text>
      <length>15</length>
      <inputType>string</inputType>
    </input>
    <input type="label" translate="false" bounds="0,1,4,1" id="label3">
      <text>Matrix-Size:</text>
    </input>
    <input type="textfield" translate="false" bounds="0,5,2,1" id="size">
      <text>default</text>
      <length>1</length>
      <inputType>number</inputType>
    </input>
  </inputmask>
</process>